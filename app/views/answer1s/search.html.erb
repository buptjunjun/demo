<form action="/answer1s/search" method="post">
   <input name="q" placeholder="search text"/>
   <input type="submit" value="submit" />
</form>

<% if @answer1s.blank? == false%>
    <%@answer1s.each do |answer|%>
        <input type="checkbox"/>
        <%=render :partial => 'single',:locals => {:answer => answer}%>
        <%=link_to 'edit',:action => 'edit',:id=>answer.id %>
        <%=link_to 'destroy',{:action => 'destroy',:id=>answer.id},class:'destroy' %>
        </br>
    <%end%>
    <%=link_to 'new',:action => 'new' %>
<%else%>
    no data
<%end %>